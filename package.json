{
  "name": "nambururu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18.16.0",
    "yarn": "1.22.19"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "concurrently \"yarn format:prettier\" \"yarn lint:fix\"",
    "format:prettier": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "concurrently \"yarn lint:js\" \"yarn lint:styles\"",
    "lint:fix": "yarn lint:js --fix",
    "lint:js": "eslint --ext=js,ts,tsx src",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "lint-staged": {
    "*.{ts,js,tsx}": [
      "yarn format",
      "yarn lint:js"
    ],
    "*.css": [
      "yarn format",
      "yarn lint:styles"
    ]
  },
  "devDependencies": {
    "@preact/preset-vite": "2.5.0",
    "@types/lodash.debounce": "4.0.7",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vite-pwa/assets-generator": "0.0.3",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-v8": "0.32.0",
    "concurrently": "8.2.0",
    "eslint": "8.42.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-preact": "1.3.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.1",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "lodash.debounce": "4.0.8",
    "prettier": "2.8.8",
    "react-intersection-observer": "9.4.4",
    "stylelint": "15.7.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended": "12.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.0.4",
    "usehooks-ts": "2.9.1",
    "vite": "4.3.9",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-pwa": "0.16.4",
    "vite-plugin-stylelint": "4.3.0",
    "vitest": "0.32.0"
  },
  "dependencies": {
    "@tanstack/react-query": "4.29.13",
    "preact": "10.15.1"
  }
}
